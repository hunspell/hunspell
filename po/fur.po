#: src/tools/hunspell.cxx:1816
msgid "Usage: hunspell [OPTION]... [FILE]...\n"
msgstr "Ûs: hunspell [OPZION]... [FILE]...\n"

#: src/tools/hunspell.cxx:1817
msgid ""
"Check spelling of each FILE. Without FILE, check standard input.\n"
"\n"
msgstr ""
"Controle la ortografie di ogni FILE. Cence FILE, controle il standard input."
"\n"
"\n"

#: src/tools/hunspell.cxx:1819
msgid "  -1\t\tcheck only first field in lines (delimiter = tabulator)\n"
msgstr ""
"  -1\t\tcontrole nome il prin cjamp tes riis (delimitadôr = tabuladôr)\n"

#: src/tools/hunspell.cxx:1821
msgid "  -a\t\tIspell's pipe interface\n"
msgstr "  -a\t\tinterface di cumbinazion (pipe) di Ispell\n"

#: src/tools/hunspell.cxx:1822
msgid "  --check-url\tcheck URLs, e-mail addresses and directory paths\n"
msgstr ""
"  --check-url\tcontrole i URLs, lis direzions e-mail e i percors des "
"cartelis\n"

#: src/tools/hunspell.cxx:1827
msgid "  --check-apostrophe\tcheck Unicode typographic apostrophe\n"
msgstr "  --check-apostrophe\tcontrole l'apostrof tipografic di Unicode\n"

#: src/tools/hunspell.cxx:1829
msgid "  -d d[,d2,...]\tuse d (d2 etc.) dictionaries\n"
msgstr "  -d d[,d2,...]\tdopre i dizionaris d (d2 e v.i.)\n"

#: src/tools/hunspell.cxx:1830
msgid "  -D\t\tshow available dictionaries\n"
msgstr "  -D\t\tmostre i dizionaris disponibii\n"

#: src/tools/hunspell.cxx:1831
msgid "  -G\t\tprint only correct words or lines\n"
msgstr "  -G\t\tstampe dome lis riis o lis peraulis justis\n"

#: src/tools/hunspell.cxx:1832
msgid "  -h, --help\tdisplay this help and exit\n"
msgstr "  -h, --help\tvisualize chest jutori e jes\n"

#: src/tools/hunspell.cxx:1833
msgid "  -H\t\tHTML input file format\n"
msgstr "  -H\t\tformât dal file di input in HTML\n"

#: src/tools/hunspell.cxx:1834
msgid "  -i enc\tinput encoding\n"
msgstr "  -i cod\tcodifiche dal input\n"

#: src/tools/hunspell.cxx:1835
msgid "  -l\t\tprint misspelled words\n"
msgstr "  -l\t\tstampe lis peraulis sbaliadis\n"

#: src/tools/hunspell.cxx:1836
msgid "  -L\t\tprint lines with misspelled words\n"
msgstr "  -L\t\tstampe lis riis cu lis peraulis sbaliadis\n"

#: src/tools/hunspell.cxx:1838
msgid "  -m \t\tanalyze the words of the input text\n"
msgstr "  -m \t\tanalize lis peraulis dal test in jentrade\n"

#: src/tools/hunspell.cxx:1839
msgid "  -n\t\tnroff/troff input file format\n"
msgstr "  -n\t\tformât nroff/troff dal file in jentrade\n"

#: src/tools/hunspell.cxx:1843
msgid "  -O\t\tOpenDocument (ODF or Flat ODF) input file format\n"
msgstr "  -n\t\tformât OpenDocument (ODF o Flat ODF) dal file in jentrade\n"

#: src/tools/hunspell.cxx:1844
msgid "  -p dict\tset dict custom dictionary\n"
msgstr "  -p diz\tstabilìs diz come dizionari personalizât\n"

#: src/tools/hunspell.cxx:1846
msgid "  -r\t\twarn of the potential mistakes (rare words)\n"
msgstr "  -r\t\tvise di erôrs potenziâi (peraulis raris)\n"

#: src/tools/hunspell.cxx:1849
msgid "  -P password\tset password for encrypted dictionaries\n"
msgstr "  -P password\tstabilìs la password pai dizionaris cifrâts\n"

#: src/tools/hunspell.cxx:1850
msgid "  -s \t\tstem the words of the input text\n"
msgstr "  -s \t\trecupere la lidrîs des peraulis dal test in jentrade\n"

#: src/tools/hunspell.cxx:1851
msgid "  -S \t\tsuffix words of the input text\n"
msgstr "  -S \t\tsufisse lis peraulis dal test in jentrade\n"

#: src/tools/hunspell.cxx:1852
msgid "  -t\t\tTeX/LaTeX input file format\n"
msgstr "  -t\t\tformât TeX/LaTeX dal file in jentrade\n"

#: src/tools/hunspell.cxx:1853
msgid "  -v, --version\tprint version number\n"
msgstr "  -v, --version\tstampe il numar de version\n"

#: src/tools/hunspell.cxx:1855
msgid "  -vv\t\tprint Ispell compatible version number\n"
msgstr "  -vv\t\tstampe il numar de version compatibile di Ispell\n"

#: src/tools/hunspell.cxx:1856
msgid "  -w\t\tprint misspelled words (= lines) from one word/line input.\n"
msgstr ""
"  -w\t\tstampe lis peraulis sbaliadis (= riis) di une peraule o rie in "
"jentrade.\n"

#: src/tools/hunspell.cxx:1858
msgid ""
"  -X\t\tXML input file format\n"
"\n"
msgstr ""
"  -H\t\tformât XML dal file in jentrade\n"
"\n"

#: src/tools/hunspell.cxx:1862
msgid ""
"Example: hunspell -d en_US file.txt    # interactive spelling\n"
"         hunspell -i utf-8 file.txt    # check UTF-8 encoded file\n"
"         hunspell -l *.odt             # print misspelled words of ODF "
"files\n"
"\n"
"         # Quick fix of ODF documents by personal dictionary creation\n"
"\n"
"         # 1 Make a reduced list from misspelled and unknown words:\n"
"\n"
"         hunspell -l *.odt | sort | uniq >words\n"
"\n"
"         # 2 Delete misspelled words of the file by a text editor.\n"
"         # 3 Use this personal dictionary to fix the deleted words:\n"
"\n"
"         hunspell -p words *.odt\n"
"\n"
msgstr ""
"Esempli: hunspell -d en_US file.txt    # control ortografic interatîf\n"
"         hunspell -i utf-8 file.txt    # controle file codificât in UTF-8\n"
"         hunspell -l *.odt             # stampe peraulis cun erôrs "
"ortografics dai files ODF\n"
"\n"
"         # Corezion svelte dai documents ODF midiant la creazion di "
"dizionaris personâi\n"
"\n"
"         # 1 Cree une liste ridote di peraulis sbaliadis e no cognossudis:\n"
"\n"
"         hunspell -l *.odt | sort | uniq >words\n"
"\n"
"         # 2 Elimine peraulis cun erôrs ortografics dal file cuntun editôr "
"di test.\n"
"         # 3 Dopre chest dizionari privât par corezi lis peraulis eliminadis:"
"\n"
"\n"
"         hunspell -p words *.odt\n"
"\n"

